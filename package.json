{
  "name": "wedding-invitation",
  "version": "0.1.0",
  "description": "This is a site with an invitation to the wedding",
  "private": true,
  "scripts": {
    "build-server": "webpack --mode production --env.configType=server",
    "build-client": "webpack --mode production --env.configType=client",
    "build": "npm run build-client && npm run build-server",
    "start": "npm run build && node dist/server.js",
    "start-dev": "webpack-dev-server --mode development --env.port=6969 --env.configType=client"
  },
  "main": "index.js",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MontyTantano/wedding-invitation.git"
  },
